{
  "name": "lumi",
  "productName": "Lumi",
  "version": "1.0.0-alpha.2",
  "description": "Lumi - Local-first personal knowledge management system with real-time collaboration",
  "repository": {
    "type": "git",
    "url": "https://github.com/nupthale/lumi.git"
  },
  "main": "main.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development electron .",
    "copy-web": "rm -rf web && mkdir -p web && cp -r ../web/dist/ web/",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make:mac": "npm run make:x64 && npm run make:arm64",
    "make:x64": "electron-forge make --arch=x64",
    "make:arm64": "electron-forge make --arch=arm64",
    "publish": "electron-forge publish"
  },
  "keywords": [],
  "author": "Lumi Team",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^17.2.2",
    "electron-log": "^5.4.3",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^8.1.0",
    "express": "^5.1.0",
    "fs-extra": "^11.3.0",
    "get-folder-size": "^5.0.0",
    "nanoid": "^5.1.5",
    "nedb": "^1.8.0",
    "node-machine-id": "^1.1.12",
    "rxjs": "^7.8.2",
    "update-electron-app": "^3.1.1",
    "which": "^5.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.3",
    "@electron-forge/maker-deb": "^7.8.3",
    "@electron-forge/maker-dmg": "^7.9.0",
    "@electron-forge/maker-rpm": "^7.8.3",
    "@electron-forge/maker-squirrel": "^7.8.3",
    "@electron-forge/maker-zip": "^7.8.3",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.3",
    "@electron-forge/plugin-fuses": "^7.8.3",
    "@electron-forge/publisher-github": "^7.8.3",
    "@electron/fuses": "^1.8.0",
    "cross-env": "^7.0.3",
    "electron": "36.3.2"
  }
}
